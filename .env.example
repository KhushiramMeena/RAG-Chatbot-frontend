# Frontend Environment Variables for Render
# Copy these to your Render service environment variables

# API Configuration (REQUIRED)
# Replace with your backend Render URL
REACT_APP_API_URL=https://your-backend-app.onrender.com/api
REACT_APP_SOCKET_URL=https://your-backend-app.onrender.com

# Application Configuration
REACT_APP_APP_NAME=News Chatbot
REACT_APP_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=production

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=false

# External Services (Optional)
REACT_APP_SENTRY_DSN=
REACT_APP_GOOGLE_ANALYTICS_ID=
